{
  "name": "raptor",
  "version": "1.0.0-alfa",
  "description": "Raptor Engine",
  "scripts": {
    "clean": "rimraf fake-cdn/ && git checkout fake-cdn",
    "start": "forever --uid raptor fake-server/server.js & node scripts/rollup.build.js --watch && forever stop raptor",
    "stop": "forever stopall",
    "build": "npm run clean && node scripts/rollup.build.js --production",
    "lint": "eslint src/",
    "pretest": "npm run lint",
    "preversion": "npm run clean && npm run build",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://git.soma.salesforce.com/raptor/raptor.git"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "eslint": "^3.0.1",
    "eslint-plugin-flowtype": "^2.7.1",
    "express": "^4.14.0",
    "forever": "^0.15.2",
    "glob": "^7.1.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.34.3",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-flow": "^1.0.1",
    "rollup-plugin-inject": "^1.4.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-raptor-compiler": "^0.0.13",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-strip": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^2.5.0",
    "serve-static": "^1.11.1",
    "snabbdom": "^0.5.3",
    "typescript-eslint-parser": "eslint/typescript-eslint-parser",
    "watch": "^1.0.0",
    "yargs": "^6.0.0"
  }
}
