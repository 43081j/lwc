{
  "name": "raptor-compiler",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepublish": "lerna bootstrap && lerna run build",
    "test": "lerna run test",
    "build": "lerna run build",
    "build:compiler": "webpack --config scripts/webpack.commonjs-module.js",
    "build:compilermin": "lerna run build && webpack --config scripts/webpack.commonjs-module.js --env.prod=true",
    "build:webcompiler": "lerna run build && webpack --config scripts/webpack.web.js",
    "clean": "lerna run clean",
    "lint": "lerna run lint",
    "preversion": "npm run lint && npm run clean && npm run build",
    "template-cli" : "./packages/babel-plugin-transform-raptor-template/scripts/cli.js",
    "compiler-cli" : "./packages/raptor-compiler-core/scripts/cli.js"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^3.13.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "lerna": "2.0.0-beta.32",
    "mocha": "^3.1.2",
    "rimraf": "^2.5.4",
    "webpack": "^2.2.0"
  },
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "string-replace-webpack-plugin": "0.0.5"
  }
}
