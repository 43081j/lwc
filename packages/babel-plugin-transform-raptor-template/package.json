{
  "name": "babel-plugin-transform-raptor-template",
  "version": "0.1.3",
  "description": "Babel plugin to transform a HTML template into Raptor format",
  "main": "dist/index.js",
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.13.1",
    "eslint-plugin-flowtype": "^2.25.0",
    "mocha": "^3.1.2",
    "power-assert": "^1.4.1",
    "rimraf": "^2.5.4",
    "raptor-helper-fixture": "^0.1.3"
  },
  "dependencies": {
    "css-parse": "^2.0.0",
    "to-camel-case": "^1.0.0"
  },
  "scripts": {
    "clean": "rimraf dist/",
    "build": "babel src/ --out-dir dist/",
    "build:fixtures": "babel-node ./scripts/build-fixtures.js",
    "test": "mocha --compilers js:babel-register",
